config BR2_PACKAGE_CRDA
	bool "central regulatory domain agent"
	select BR2_PACKAGE_LIBNL
	select BR2_PACKAGE_OPENSSL
	select BR2_PACKAGE_UDEV
	select BR2_PACKAGE_WIRELESS_REGDB
