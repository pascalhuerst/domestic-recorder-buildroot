+/*
+ * Return the current NAND controller clock frequency
+ */
+unsigned long clk_pxa3xx_nand_getrate(struct clk *clk)
+{
+	unsigned long acsr = ACSR;
+
+	if (acsr & ACCR_D0CS)
+		/* ring oscillator freq: 30 MHz */
+		return 30000000;
+
+	if (cpu_is_pxa300() || cpu_is_pxa310())
+		/* 156MHz */
+		return 156000000;
+
+	if (cpu_is_pxa320())
+		/* 104MHz */
+		return 104000000;
+
+	return 0;
+}
