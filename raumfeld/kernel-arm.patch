+		GPIO106_GPIO,
+	};
+
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_controller_pin_config));
+	platform_add_devices(ARRAY_AND_SIZE(raumfeld_controller_devices));
+	spi_register_board_info(ARRAY_AND_SIZE(controller_spi_devices));
+	i2c_register_board_info(0, &raumfeld_controller_i2c_board_info, 1);
+
+	gpio_request(mfp_to_gpio(GPIO_SHUTDOWN_BATT), "battery shutdown");
+	gpio_direction_output(mfp_to_gpio(GPIO_SHUTDOWN_BATT), 0);
+
+	raumfeld_common_init();
+	raumfeld_power_init();
+	raumfeld_lcd_init();
+	raumfeld_w1_init();
+}
+
+static void __init raumfeld_connector_init(void)
+{
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_ssp1_pin_config));
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_ssp2_pin_config));
+	spi_register_board_info(ARRAY_AND_SIZE(connector_spi_devices));
+	i2c_register_board_info(0,
+			ARRAY_AND_SIZE(raumfeld_connector_i2c_board_info));
+
+	raumfeld_common_init();
+	raumfeld_eth_init();
+
+	raumfeld_claim_audio_gpios();
+	raumfeld_enable_audio(true);
+}
+
+static void __init raumfeld_speaker_init(void)
+{
+	struct platform_device *raumfeld_speaker_devices[] = {
+		&rotary_encoder_device,
+	};
+
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_ssp1_pin_config));
+	platform_add_devices(ARRAY_AND_SIZE(raumfeld_speaker_devices));
+	spi_register_board_info(ARRAY_AND_SIZE(speaker_spi_devices));
+	i2c_register_board_info(0,
+			ARRAY_AND_SIZE(raumfeld_connector_i2c_board_info));
+
+	raumfeld_common_init();
+	raumfeld_eth_init();
+
+	raumfeld_claim_audio_gpios();
+	raumfeld_enable_audio(true);
+}
+
+static struct i2c_board_info raumfeld_proto_i2c_board_info[] __initdata = {
+	{
+		.type	= "cs4270",
+		.addr	= 0x48,
+	},
+	{
+		.type	= "eeti_ts",
+		.addr	= 0x0a,
+		.irq	= gpio_to_irq(mfp_to_gpio(GPIO_TOUCH_IRQ)),
+		.platform_data = &eeti_ts_pdata,
+	}
+};
+
+static void __init raumfeld_proto_init(void)
+{
+	struct platform_device *raumfeld_proto_devices[] = {
+		&rotary_encoder_device,
+	};
+
+	mfp_cfg_t raumfeld_proto_pin_config[] = {
+		GPIO25_GPIO | MFP_PULL_HIGH,
+		GPIO26_GPIO | MFP_PULL_HIGH,
+		GPIO28_GPIO,
+		GPIO29_GPIO,
+	};
+
+	gpio_request(mfp_to_gpio(GPIO_DEBUG_LED), "gpio led");
+	gpio_direction_output(mfp_to_gpio(GPIO_DEBUG_LED), 0);
+
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_ssp1_pin_config));
+	pxa3xx_mfp_config(ARRAY_AND_SIZE(raumfeld_proto_pin_config));
+	platform_add_devices(ARRAY_AND_SIZE(raumfeld_proto_devices));
+	spi_register_board_info(ARRAY_AND_SIZE(proto_spi_devices));
+	i2c_register_board_info(0, ARRAY_AND_SIZE(raumfeld_proto_i2c_board_info));
+
+	raumfeld_common_init();
+	raumfeld_eth_init();
+	raumfeld_lcd_init();
+	raumfeld_w1_init();
+
+	raumfeld_claim_audio_gpios();
+	raumfeld_enable_audio(true);
+}
