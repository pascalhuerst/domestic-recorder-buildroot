+#endif
+
+#ifdef CONFIG_MACH_RAUMFELD_PROTO
+MACHINE_START(RAUMFELD_PROTO, "Raumfeld prototype")
+	.phys_io	= 0x40000000,
+	.io_pg_offst	= (io_p2v(0x40000000) >> 18) & 0xfffc,
