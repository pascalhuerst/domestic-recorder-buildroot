#!/bin/sh

. ../tests.inc
TITLE="AUDIOLOOPBACK"
DIALOGOPTS="--title $TITLE"

/raumfeld/p0-audiotest/p0-audiotest | \
	/percent 216 | \
	dialog_progress "Testing audio loopback. Please wait." $DIALOGOPTS

if [ "$?" = "0" ]; then
	dialog_success "OK!" $DIALOGOPTS
else
	dialog_err "TEST FAILED!" $DIALOGOPTS
	exit 1
fi

